{
  "_comment": "See http://www.chromium.org/chromium-os/building-chromium-os/disk-layout-format",
  "metadata":{
    "rootdev_base": "/sys/devices/pci0000:00/0000:00:17.0/ata*/host*/target*/*/block/sd* /sys/devices/pci0000:00/0000:00:1c.*/0000:*:00.0/nvme/nvme0/nvme0n1"
  },
  "parent": "legacy_disk_layout.json",
  "layouts": {
    # common is the standard layout template.
    "common": [
      {
        # Special partition for firmware dignostics data
        "num": 9,
        "size": "1 MiB",
        "fs_format": "vfat",
        "label": "DIAGNOSTICS",
        "type": "data",
        "uuid": "clear"
      }
    ],
    "base": [
      {
        # Slot B rootfs. Updated for more space, like Slot A below
        "num": 5,
        "size": "4 GiB"
      },
      {
        # Slot A rootfs. Adjust to allow for more space.
        "num": 3,
        "fs_size_min": "2300 MiB",
        "size": "4 GiB"
      },
      {
        # 4GB stateful partition to store temporary files during testing.
        "num": 1,
        "size": "4 GiB"
      }
    ],
    "usb": [
      {
        "num": 3,
        "fs_size_min": "2300 MiB",
        "size": "2400 MiB"
      }
    ],
    "usb-updatable": [
      {
        "num": 5,
        "size": "2400 MiB"
      },
      {
        "num": 3,
        "fs_size_min": "2300 MiB",
        "size": "2400 MiB"
      }
    ],
    # Used for recovery images.
    "recovery": [
      {
        # Kernel for Slot A, no file system.
        # Make the partition as large as the firmware supports so we can boot
        # larger recovery kernels.  The kernel won't actually be this large.
        "num": 2,
        "size": "512 MiB"
      },
      {
        "num": 3,
        "fs_size_min": "2300 MiB",
        "size": "2400 MiB"
      }
    ]
  }
}
