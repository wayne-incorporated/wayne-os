group("all") {
  deps = [ ":install_init_scripts" ]
}

install_config("install_init_scripts") {
  sources = [
    "init/arcvm-media-sharing-services.conf",
    "init/arcvm-mount-myfiles.conf",
    "init/arcvm-mount-removable-media.conf",
  ]
  install_path = "/etc/init"
}
