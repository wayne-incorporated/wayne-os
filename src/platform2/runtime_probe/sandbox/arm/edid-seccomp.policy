# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

ARM_set_tls: 1
_llseek: 1
access: 1
brk: 1
clock_gettime64: 1
clock_gettime: 1
close: 1
connect: 1
exit: 1
exit_group: 1
faccessat: 1
faccessat2: 1
fstat64: 1
fstatat64: 1
fstatfs64: 1
fstatfs: 1
getdents64: 1
gettimeofday: 1
lstat64: 1
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munmap: 1
openat: 1
read: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
rt_sigreturn: 1
send: 1
set_robust_list: 1
set_tid_address: 1
socket: arg0 == AF_UNIX
stat64: 1
statx: 1
ugetrlimit: 1
uname: 1
write: 1

# For brillo::kLogToStderrIfTty. isatty(0) calls ioctl(0, TCGETS, *).
ioctl: arg0 == 0 && arg1 == TCGETS
getpid: 1
futex: 1
futex_time64: 1
