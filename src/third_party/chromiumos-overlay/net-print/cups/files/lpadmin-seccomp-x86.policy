# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

_llseek: 1
access: 1
bind: 1
brk: 1
clock_gettime: 1
clock_gettime64: 1
close: 1
connect: 1
exit: 1
exit_group: 1
faccessat: 1
faccessat2: 1
fcntl64: 1
fstat64: 1
fstatfs64: 1
fstatfs: 1
futex: 1
futex_time64: 1
getdents: 1
getdents64: 1
getegid32: 1
geteuid32: 1
getgid32: 1
getpeername: 1
getpid: 1
getrandom: 1
getrusage: 1
getsockname: 1
gettid: 1
gettimeofday: 1
getuid32: 1
ioctl: 1
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mremap: 1
munmap: 1
nanosleep: 1
clock_nanosleep: 1
clock_nanosleep_time64: 1
open: 1
openat: 1
poll: 1
read: 1
recvmsg: 1
rt_sigaction: 1
setgroups32: 1
setresgid32: 1
setresuid32: 1
setsockopt: 1
# AF_UNIX = 1, AF_INET = 2, AF_INET6 = 10
socket: arg0 == 1 || arg0 == 2 || arg0 == 10
# SYS_SOCKET = 1, SYS_CONNECT = 3, SYS_GETPEERNAME = 7, SYS_SEND = 9,
# SYS_RECV = 10, SETSOCKOPT = 14
socketcall: arg0 == 1 || arg0 == 3 || arg0 == 7 || arg0 == 9 || arg0 == 10 || arg0 == 14
stat64: 1
time: 1
uname: 1
unlink: 1
write: 1
writev: 1
