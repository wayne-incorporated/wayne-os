CONFIG_VENDOR_INTEL=y
CONFIG_BOARD_INTEL_TGLRVP_UP4=y

CONFIG_USE_BLOBS=y
CONFIG_VENDOR_INTEL=y
CONFIG_CHROMEOS=y
CONFIG_MAINBOARD_SMBIOS_MANUFACTURER="Intel"
CONFIG_CBFS_SIZE=0x300000

CONFIG_SPI_FLASH_SMM=y
CONFIG_DO_NOT_TOUCH_DESCRIPTOR_REGION=y

# FSP Blobs
CONFIG_FSP_HEADER_PATH="3rdparty/blobs/intel/tgl/fsp/Include"
CONFIG_ADD_FSP_BINARIES=y
CONFIG_FSP_M_FILE="3rdparty/blobs/intel/tgl/fsp/fspm.bin"
CONFIG_FSP_S_FILE="3rdparty/blobs/intel/tgl/fsp/fsps.bin"

CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/intel/tglrvp/rvp_y/es2/descriptor.bin"
CONFIG_HAVE_IFD_BIN=y
CONFIG_HAVE_ME_BIN=y
CONFIG_ME_BIN_PATH="3rdparty/blobs/mainboard/intel/tglrvp/rvp_y/es2/me.bin"
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/intel/tglrvp/vbt.bin"
CONFIG_DISPLAY_FSP_CALLS_AND_STATUS=y
CONFIG_DISPLAY_FSP_HEADER=y
CONFIG_FSP_M_XIP=y
CONFIG_PAYLOAD_NONE=y
CONFIG_CPU_MICROCODE_CBFS_EXTERNAL_BINS=y
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/intel/tglrvp/rvp_y/ucode.bin"
CONFIG_FMDFILE="src/mainboard/intel/tglrvp/chromeos.fmd"
CONFIG_GBB_FLAG_FORCE_DEV_SWITCH_ON=n
CONFIG_RUN_FSP_GOP=y
# CONFIG_CONSOLE_SERIAL is not set
CONFIG_INTEL_GMA_ADD_VBT=yw

# Event Logging
CONFIG_CMOS_POST=y
CONFIG_CMOS_POST_EXTRA=y
CONFIG_CMOS_POST_OFFSET=0x70
CONFIG_ELOG=y
CONFIG_ELOG_GSMI=y
CONFIG_ELOG_BOOT_COUNT=y
CONFIG_ELOG_BOOT_COUNT_CMOS_OFFSET=144
