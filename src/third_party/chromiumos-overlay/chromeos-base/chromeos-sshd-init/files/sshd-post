# For good hygiene, clean up on job stop.
for cmd in iptables ip6tables ; do
# Modified by seongbin@wayne-inc.com
#  $cmd -D INPUT -p tcp --dport 22 -j ACCEPT -w || true
  $cmd -D INPUT -p tcp --dport 22222 -j ACCEPT -w || true
done
